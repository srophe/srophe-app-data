                    <persName xml:id="name1836-4" xml:lang="en">Joseph the Patriarch</persName>
                    <persName xml:id="name1836-5" xml:lang="en" source="#bib1836-1">Joseph, Patriarch</persName>
                    <persName xml:id="name1836-6" xml:lang="fr-x-zanetti" source="#bib1836-1">Yawsef</persName>
                    <persName xml:id="name1836-7" xml:lang="fr-x-zanetti" source="#bib1836-1">Yawsep</persName>
